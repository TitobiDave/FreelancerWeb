{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Freelancer": "Server=TITO; Initial Catalog=FreelancerDB; trustServerCertificate=true; Trusted_Connection=true"
  },
